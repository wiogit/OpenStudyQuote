{
  "manifest_version": 2,
  
  "name": "Open Study Quoting",
  "description": "Quote posts on Open Study",
  "version": "1.3",
  
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" 
  },
  
  "content_scripts": [
    {
      "matches": ["http://openstudy.com/*"],
      "css" : ["quote.css"],
      "js": ["jquery.js", "quote.js", "interface.js", "inject.js"]
    }
  ]
}